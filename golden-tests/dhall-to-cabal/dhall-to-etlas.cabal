source-repository head
    type: git
    location: https://github.com/eta-lang/dhall-to-etlas

library
    exposed-modules:
        DhallToCabal
        DhallLocation
        CabalToDhall
    build-depends:
        etlas-cabal >=1.3.0.0 && <1.4,
        base >=4.5 && <5,
        bytestring >=0.10 && <0.11,
        containers >=0.5 && <0.6,
        contravariant >=1.4 && <1.5,
        dhall >=1.15.0 && <1.16,
        hashable >=1.2.6.1 && <1.3,
        insert-ordered-containers >=0.2.1.0 && <0.3,
        text >=1.2 && <1.3,
        transformers >=0.2.0.0 && <0.6,
        vector >=0.11.0.0 && <0.13,
        semigroups >=0.18.0 && <0.19
    default-language: Haskell2010
    other-extensions: GADTs GeneralizedNewtypeDeriving LambdaCase
                      OverloadedStrings RecordWildCards
    hs-source-dirs: lib
    other-modules:
        DhallToCabal.ConfigTree
        DhallToCabal.Diff
        Dhall.Extra
        Paths_dhall_to_etlas
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

executable  dhall-to-etlas
    main-is: Main.hs
    build-depends:
        etlas-cabal >=1.3.0.0 && <1.4,
        base >=4.5 && <5,
        dhall >=1.15.0 && <1.16,
        dhall-to-etlas -any,
        insert-ordered-containers >=0.2.1.0 && <0.3,
        optparse-applicative >=0.13.2 && <0.15,
        prettyprinter >=1.2.0.1 && <1.3,
        text >=1.2 && <1.3,
        transformers >=0.2.0.0 && <0.6
    default-language: Haskell2010
    other-extensions: NamedFieldPuns
    hs-source-dirs: exe
    other-modules:
        Paths_dhall_to_cabal
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

executable  etlas-to-dhall
    main-is: Main.hs
    build-depends:
        base >=4.5 && <5,
        dhall >=1.15.0 && <1.16,
        bytestring >=0.10 && <0.11,
        dhall-to-etlas -any,
        optparse-applicative >=0.13.2 && <0.15,
        prettyprinter >=1.2.0.1 && <1.3,
        text >=1.2 && <1.3
    default-language: Haskell2010
    other-extensions: NamedFieldPuns
    hs-source-dirs: cabal-to-dhall
    other-modules:
        Paths_dhall_to_etlas
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

