name: dhall-to-etlas
-- * * * * * * * * * * * * WARNING * * * * * * * * * * * *
-- This file has been AUTO-GENERATED by dhall-to-etlas.
--
-- Do not edit it by hand, because your changes will be over-written!
--
-- Instead, edit the source Dhall file, namely
-- 'dhall-to-etlas.dhall', and re-generate this file by running
-- 'dhall-to-etlas -- dhall-to-etlas.dhall'.
-- * * * * * * * * * * * * WARNING * * * * * * * * * * * *
version: 1.2.0.0
cabal-version: 1.12
build-type: Simple
license: MIT
license-file: LICENSE
maintainer: atreyu.bbb@gmail.com
homepage: https://github.com/eta-lang/dhall-to-etlas
bug-reports: https://github.com/eta-lang/dhall-to-etlas/issues
synopsis: Compile Dhall expressions to Etlas files
description:
    dhall-to-etlas takes Dhall expressions and compiles them into Etlas
    files. All of the features of Dhall are supported, such as let
    bindings and imports, and all features of Etlas are supported
    (including conditional stanzas).
    .
category: Distribution
author: Ollie Charles <ollie@ocharles.org.uk>
extra-source-files:
    Changelog.md
    README.md
    dhall/*.dhall
    dhall/defaults/*.dhall
    dhall/Dependency/*.dhall
    dhall/SPDX/*.dhall
    dhall/types/*.dhall
    dhall/types/SPDX/*.dhall
    dhall/utils/*.dhall
    dhall/Version/*.dhall
    dhall/VersionRange/*.dhall
    golden-tests/dhall-to-cabal/*.dhall
    golden-tests/dhall-to-cabal/*.cabal
    golden-tests/cabal-to-dhall/*.dhall
    golden-tests/cabal-to-dhall/*.cabal

source-repository head
    type: git
    location: https://github.com/eta-lang/dhall-to-etlas

library
    exposed-modules:
        DhallToCabal
        DhallLocation
        CabalToDhall
    build-depends:
        etlas-cabal >=1.3.0.0 && <1.4,
        base >=4.5 && <5,
        bytestring >=0.10 && <0.11,
        containers >=0.5 && <0.6,
        contravariant >=1.4 && <1.5,
        dhall >=1.23.0 && <1.24,
        hashable >=1.2.6.1 && <1.3,
        insert-ordered-containers >=0.2.1.0 && <0.3,
        text >=1.2 && <1.3,
        transformers >=0.2.0.0 && <0.6,
        vector >=0.11.0.0 && <0.13,
        semigroups >=0.18.0 && <0.19
    default-language: Haskell2010
    other-extensions: GADTs GeneralizedNewtypeDeriving LambdaCase
                      OverloadedStrings RecordWildCards
    hs-source-dirs: lib
    other-modules:
        DhallToCabal.ConfigTree
        DhallToCabal.Diff
        Dhall.Extra
        Paths_dhall_to_etlas
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

executable  dhall-to-etlas
    main-is: Main.hs
    build-depends:
        etlas-cabal >=1.3.0.0 && <1.4,
        base >=4.5 && <5,
        dhall >=1.23.0 && <1.24,
        dhall-to-etlas -any,
        insert-ordered-containers >=0.2.1.0 && <0.3,
        optparse-applicative >=0.13.2 && <0.15,
        prettyprinter >=1.2.0.1 && <1.3,
        text >=1.2 && <1.3,
        transformers >=0.2.0.0 && <0.6
    default-language: Haskell2010
    other-extensions: NamedFieldPuns
    hs-source-dirs: exe
    other-modules:
        Paths_dhall_to_cabal
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

executable  etlas-to-dhall
    main-is: Main.hs
    build-depends:
        base >=4.5 && <5,
        dhall >=1.23.0 && <1.24,
        bytestring >=0.10 && <0.11,
        dhall-to-etlas -any,
        optparse-applicative >=0.13.2 && <0.15,
        prettyprinter >=1.2.0.1 && <1.3,
        text >=1.2 && <1.3
    default-language: Haskell2010
    other-extensions: NamedFieldPuns
    hs-source-dirs: cabal-to-dhall
    other-modules:
        Paths_dhall_to_etlas
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

executable  dhall-to-cabal-meta
    main-is: Main.hs
    build-depends:
        base >=4.5 && <5,
        directory >=1.3.0.2 && <1.4,
        dhall >=1.23.0 && <1.24,
        dhall-to-etlas -any,
        filepath >=1.4 && <1.5,
        optparse-applicative >=0.13.2 && <0.15,
        prettyprinter >=1.2.0.1 && <1.3
    default-language: Haskell2010
    hs-source-dirs: meta
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing

test-suite  golden-tests
    type: exitcode-stdio-1.0
    main-is: GoldenTests.hs
    build-depends:
        base >=4.5 && <5,
        etlas-cabal >=1.3.0.0 && <1.4,
        Diff >=0.3.4 && <0.4,
        bytestring >=0.10 && <0.11,
        dhall >=1.23.0 && <1.24,
        dhall-to-etlas -any,
        filepath >=1.4 && <1.5,
        prettyprinter >=1.2.0.1 && <1.3,
        tasty >=0.11 && <1.3,
        tasty-golden >=2.3 && <2.4,
        text >=1.2 && <1.3
    default-language: Haskell2010
    hs-source-dirs: golden-tests
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing
test-suite  unit-tests
    type: exitcode-stdio-1.0
    main-is: Tests.hs
    build-depends:
        base >=4.5 && <5,
        etlas-cabal >=1.3.0.0 && <1.4,
        dhall >=1.23.0 && <1.24,
        dhall-to-cabal -any,
        tasty >=0.11 && <1.3,
        tasty-hunit >=0.10.0.1 && <0.11,
        text >=1.2 && <1.3
    default-language: Haskell2010
    default-extensions: BangPatterns DataKinds DeriveFoldable
                        DeriveFunctor DeriveGeneric DeriveTraversable EmptyCase
                        ExistentialQuantification FlexibleContexts FlexibleInstances
                        FunctionalDependencies GeneralizedNewtypeDeriving MagicHash
                        MultiParamTypeClasses MultiWayIf LambdaCase OverloadedStrings
                        RankNTypes RecordWildCards StandaloneDeriving ScopedTypeVariables
                        TupleSections TypeFamilies TypeOperators
    hs-source-dirs: tests
    other-modules:
        DhallToCabal.Tests
    ghc-prof-options: -Wall -fwarn-incomplete-uni-patterns -fwarn-incomplete-record-updates
    ghc-shared-options: -Wall -fwarn-incomplete-uni-patterns -fwarn-incomplete-record-updates
    ghc-options: -Wall -fno-warn-safe -fno-warn-unsafe -fno-warn-implicit-prelude -fno-warn-missing-import-lists -fno-warn-missing-local-sigs -fno-warn-monomorphism-restriction -fno-warn-name-shadowing
